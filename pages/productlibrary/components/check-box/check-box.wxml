<!--pages/productlibrary/components/check-box/check-box.wxml-->
<view class="check-box" bindtap="onSelected">
    <image class="select-image" src="/assets/images/group/{{checked ? 'pro_lib_select.png': 'pro_lib_unselect.png'}}" />
    <text class="text">{{checked ? '已选择' : '选择'}}</text>
</view>
