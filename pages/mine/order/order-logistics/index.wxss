.logsLeft {
    width: 75%;
    white-space: nowrap; 
    text-overflow: ellipsis;
    overflow: hidden;
    display: flex;
    height: 80rpx;
    align-items: center;
}
.logsRight {
    border: 1rpx solid #f4f4f4;
    padding:0 10rpx;
    height: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
}
.iconfont {
    font-family: "iconfont";
    font-size: 40rpx;
    text-align: center;
    color: #07c160;
}
.icon-bianji1 {
    font-size: 30rpx;
}
.logisticsNumber {
    /* max-width: 30%; */
    white-space: nowrap; 
    text-overflow: ellipsis;
    overflow: hidden;
    flex: 0 0 180rpx;
    font-size: 28rpx;
}
.logisticsIcon {
    margin-left: 20rpx;
    line-height: 80rpx;
}