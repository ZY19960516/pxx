::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}
.warn {
  background: #FAD4D0;
  text-align: center;
  color: #FA2913;
  margin: 0 auto;
  font-size: 24rpx;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.content {
  background-color: #f5f5f5;
  width: 100%;
  min-height: 100vh;
  box-sizing: border-box;
  overflow-y: scroll;
  position: relative;
  padding-bottom: 60rpx;
}



