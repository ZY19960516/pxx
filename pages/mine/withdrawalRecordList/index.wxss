/* 提现记录 */
page {
  height: 100%;
  width: 100%;
  background-color: #F4F6F9;
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
  display: none;
  }
.content {
  background-color: #f5f5f5;
  width: 100%;
  height: 100%;
  overflow-y: scroll;
}
.iconfont {
  font-family: "iconfont";
  font-size: 30rpx;
  text-align: center;
  flex: 1;
}
.iconColorget {
  color: #589dff;
}
.iconColorsend {
  color: #ff930e;
}
.listBox {
  display: flex;
  justify-content: space-between;
  background: #ffffff;
  color: #333333;
  font-size: 28rpx;
  padding: 20rpx;
  align-items: center;
  height: 120rpx;
  border-bottom: 1px solid #f4f4f4;
}
.amount {
  flex: 1;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: inline-block;
  text-align: right;
}
.centerBox {
  display: flex;
  flex-flow: column;
  align-items: flex-start;
  width: 60%;
}
.centerBox text {
  line-height: 60rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: inline-block;
  width: 100%;
  text-align: left;
}
.noData {
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  margin: auto;
  height: 280rpx;
  display: flex;
  flex-direction: column;
}
.icon-meiyoudingdan {
  font-size: 200rpx;
}